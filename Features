(START()(D(D()(DEF()(main()(B()(OP(OP(OP(OP()(RETURN()(0)))(OUTPUT()(Гонец)))('='(Гонец)(CALL(,()(Цезарь))(Галльская_Война))))(INPUT()(Цезарь)))))))(DEF(,()(Цезарь))(Галльская_Война()(B()(OP(OP(OP()(RETURN()(*(Цезарь)(CALL(,()(-(Цезарь)(1)))(Галльская_Война)))))(IF(ELESS(Цезарь)(Галлы))(B()(OP()(RETURN()(1))))))('='(Галлы)(0))))))))